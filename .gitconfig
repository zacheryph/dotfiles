[alias]
  co = checkout
  st = status
  ci = commit -s
  b = branch -v -a
  mb = merge-base
  cp = cherry-pick
  l = log --graph --decorate
  ru = remote update --prune
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative

[color]
  branch = true
  diff = true
  status = true

[credential "https://git-codecommit.us-east-2.amazonaws.com"]
  helper = ""
  helper = !aws codecommit credential-helper $@
  UseHttpPath = true

[credential]
  helper = "store --file ~/.gitcredential"

[diff]
  renames = copies

[user]
  email = zacheryph@gmail.com
  name = Zachery Hostens
  signingkey = 0x37CDFB272575E480
  username = zacheryph

[commit]
  gpgsign = true

[merge]
  log = true

[mergetool]
  keepBackup = true

[push]
  default = current

[core]
  packedGitLimit = 50m
  quotepath = false
  excludesfile = ~/.gitignore
  whitespace = space-before-tab,-indent-with-non-tab,trailing-space

[url "https://github.com/"]
  insteadOf = "hub:"

[url "git@github.com:"]
  pushInsteadOf = "hub:"
  pushInsteadOf = "https://github.com/"

[url "https://gitlab.com/"]
  insteadOf = "lab:"

[url "git@gitlab.com:"]
  pushInsteadOf = "lab:"
  pushInsteadOf = "https://gitlab.com/"

[url "https://git.zro.io/"]
  insteadOf = "zro:"
  pushInsteadOf = "zro:"

[url "https://aur.archlinux.org/"]
  insteadOf = "aur:"
  pushInsteadOf = "aur:"
